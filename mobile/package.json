{
  "name": "@ankr/interact-mobile",
  "version": "1.0.0",
  "description": "ANKR Interact — Mobile (Expo) — Offline-first knowledge OS",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:apk": "eas build -p android --profile production",
    "build:aab": "eas build -p android --profile store",
    "build:ios": "eas build -p ios --profile production",
    "build:preview": "eas build -p android --profile preview",
    "submit:android": "eas submit -p android",
    "submit:ios": "eas submit -p ios",
    "update": "eas update",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "expo": "~52.0.0",
    "expo-router": "~4.0.0",
    "expo-sqlite": "~15.0.0",
    "expo-file-system": "~18.0.0",
    "expo-notifications": "~0.29.0",
    "expo-background-fetch": "~13.0.0",
    "expo-task-manager": "~12.0.0",
    "expo-network": "~7.0.0",
    "expo-secure-store": "~14.0.0",
    "expo-sharing": "~12.0.0",
    "expo-document-picker": "~13.0.0",
    "expo-image-picker": "~16.0.0",
    "expo-av": "~14.0.0",
    "expo-speech": "~13.0.0",
    "expo-haptics": "~14.0.0",
    "expo-status-bar": "~2.0.0",
    "expo-constants": "~17.0.0",
    "expo-linking": "~7.0.0",
    "react": "18.3.2",
    "react-native": "0.76.0",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.0.0",
    "react-native-gesture-handler": "~2.20.0",
    "react-native-reanimated": "~3.16.0",
    "react-native-markdown-display": "^7.0.2",
    "react-native-svg": "15.8.0",
    "drizzle-orm": "^0.38.0",
    "drizzle-kit": "^0.30.0",
    "jszip": "^3.10.1",
    "zustand": "^5.0.0",
    "zod": "^3.23.0",
    "date-fns": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@types/react": "~18.3.12",
    "@types/react-native": "~0.76.0",
    "typescript": "~5.3.0"
  }
}
